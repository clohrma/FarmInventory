<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" styleClass="mainFxmlClass" 
            xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.VisitReportsController">
   <stylesheets>
      <URL value="@farmInventory.css" />
   </stylesheets>
   <children>
      <TableView fx:id="tableDisplay" layoutX="5.0" layoutY="55.0" prefHeight="400.0" prefWidth="790.0">
         <columns>
            <TableColumn fx:id="tblColName" prefWidth="150.0" text="Name" />
            <TableColumn fx:id="tblColDateOfBirth" minWidth="5.0" prefWidth="120.0" text="Date of Birth" />
            <TableColumn fx:id="tblColGender" minWidth="5.0" prefWidth="100.0" text="Gender" />
            <TableColumn fx:id="tblColBreed" minWidth="5.0" prefWidth="150.0" text="Breed" />
            <TableColumn fx:id="tblColColor" prefWidth="150.0" text="Color" />
            <TableColumn fx:id="tblColType" prefWidth="100.0" text="Type" />
         </columns>
      </TableView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="5.0" layoutY="5.0" prefHeight="40.0" prefWidth="790.0" styleClass="title" text="Animal Report" />
      <HBox alignment="CENTER" layoutX="5.0" layoutY="460.0" prefHeight="30.0" prefWidth="790.0" spacing="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onActionHome" prefHeight="30.0" prefWidth="150.0" styleClass="homedBtn" text="Home" />
            <Button mnemonicParsing="false" onAction="#onActionReportsMenu" prefHeight="30.0" prefWidth="150.0" styleClass="reportMenuBtn" text="Reports Menu" />
         </children>
      </HBox>
   </children>
</AnchorPane>
